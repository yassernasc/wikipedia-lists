{
  "manifest_version": 3,
  "name": "Wikipedia Lists",
  "version": "0.1",
  "description": "Use wikipedia reading lists, just like in the apps",
  "icons": { "48": "assets/icon_48x48.png", "128": "assets/icon_128x128.png" },
  "action": {
    "default_icon": "assets/icon_48x48.png",
    "default_title": "Wikipedia Lists",
    "default_popup": "index.html"
  },
  "host_permissions": ["*://*.wikipedia.org/"]
}
